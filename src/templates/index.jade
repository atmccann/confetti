        .projects
            -var projects = utils.sortByDate(data);
            for project, i in projects
                if project.shape == 'letter'
                    .project.letter(class="#{project.position}") #{project.title}

                if project.shape === 'circle'
                    a(href="#{project.url}" target="_blank")
                        .project.cir(style="background-image: url('#{project.img}');" + "background-position: 50% 50%;" class="#{project.color}")
                        
                if project.shape === 'square'
                    a(href="#{project.url}" target="_blank")
                        .project.sq(style="background-image: url('#{project.img}');" + "background-position: 50% 50%;" class="#{project.color}")


                if project.shape === 'triangle'
                    a(href="#{project.url}" target="_blank")
                        .project.tr
                            .one(class="#{project.color}")
                                .two(style="background-image: url('#{project.img}');" + "background-position: 50% 50%;")

        .btm
